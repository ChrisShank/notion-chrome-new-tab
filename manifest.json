{
  "name": "Notion New Tab",
  "version": "1.0.0",
  "description": "Launches notion.so when you open a new tab in Chrome.",
  "manifest_version": 2,
  "author": "Chris Shank",
  "browser_action": {
    "default_icon": "logo.png",
    "default_title": "Redirecting to notion.so..."
  },
  "chrome_url_overrides": {
    "newtab": "new-tab.html"
  },
  "permissions": ["activeTab"]
}
