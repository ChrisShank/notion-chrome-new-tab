{
  "name": "Notion Chrome New Tab",
  "version": "0.1",
  "description": "Open Notion.so when you open a new tab in Chrome.",
  "manifest_version": 2,
  "author": "Chris Shank",
  "browser_action": {
    "default_icon": "logo.png",
    "default_title": "Redirecting to Notion.so..."
  },
  "chrome_url_overrides": {
    "newtab": "new-tab.html"
  },
  "permissions": ["activeTab"]
}
